{
  "name": "flywheel-data-service",
  "version": "1.0.0",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "compile": "tsc",
    "start": "npm run compile && node ./dist/server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "husky",
    "lint": "eslint --fix {src,test}/**/*.{js,ts,jsx,tsx} --no-error-on-unmatched-pattern",
    "style": "prettier --write {src,test}/**/* ./*.{json,*.json} !package-lock.json -u --no-error-on-unmatched-pattern"
  },
  "lint-staged": {
    "{src,test}/**/*.{js,ts,jsx,tsx}": [
      "eslint --fix",
      "prettier --write -u"
    ],
    "!**/*.{js,ts,jsx,tsx}": "prettier --write -u"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@apollo/server": "^4.10.4",
    "graphql": "^16.8.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.4.0",
    "@types/node": "^20.14.2",
    "eslint": "^9.4.0",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.3.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.5",
    "prettier": "3.3.1",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.12.0"
  }
